{
  "compilerOptions": {
    "baseUrl": "./workspaces",
    "outDir": "./dist",
    "target": "ES2020",
    "lib": [
      "ES2020"
    ],
    "moduleResolution": "node",
    "sourceMap": true,
    "allowJs": false,
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "noImplicitReturns": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noEmit": false,
    "alwaysStrict": true,
    "declaration": true,
    "declarationMap": true,
    "strict": true,
    "strictNullChecks": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
  }
}
